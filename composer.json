{
    "name": "lmd-code/fpdf-grid-areas",
    "description": "FPDF Grid Areas - CSS Grid inspired named grid areas extension for the FPDF Library",
    "type": "library",
    "autoload": {
        "psr-4": {
            "lmdcode\\fpdfgridareas\\": "src/"
        }
    },
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "*",
        "phpcompatibility/php-compatibility": "dev-develop",
        "bartlett/php-compatinfo": "^7.1"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}